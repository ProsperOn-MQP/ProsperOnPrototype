{
  "name": "prosperonprototype",
  "version": "1.0.0",
  "scripts": {
    "install:all": "npm install && cd app/frontend && npm install && cd ../backend && npm install",
    "client": "cd app/frontend && npm run dev",
    "server": "cd app/backend && npm run build && node .",
    "python": "cd app/backend/src/python && python app.py",
    "app": "concurrently \"npm run client\" \"npm run server\"",
    "build": "concurrently \"cd app/backend && npm run build\" \"cd app/frontend && npm run build \"",
    "start": "concurrently \"cd app/backend && node .\" \"cd app/frontend && npm run host  \""
  },
  "author": "Andy Truong",
  "license": "ISC",
  "description": "Specifies dependencies for the entire application",
